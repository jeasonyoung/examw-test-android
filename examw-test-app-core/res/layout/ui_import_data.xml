<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >
	<Button
        android:id="@+id/cleanData"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="清空数据" >
    </Button>
    <Button
        android:id="@+id/importProduct"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="导入产品数据" >
    </Button>

    <Button
        android:id="@+id/importPaper"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="导入试卷数据" >
    </Button>

    <Button
        android:id="@+id/importSyllabus"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="导入大纲数据" >
    </Button>
	
    <Button
        android:id="@+id/importAll"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="导入全部数据" >
    </Button>
   <!--  <Button 
        android:id="@+id/test"
         android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="测试解析xml" /> -->
</LinearLayout>